<xs:schema 
   targetNamespace="http://informatik.unihalle.de/MiToBo_xml"
   xmlns="http://informatik.unihalle.de/MiToBo_xml"
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   elementFormDefault="qualified"
   attributeFormDefault="unqualified"
>

<xs:import namespace="http://informatik.unihalle.de/MiToBo_xml"
	schemaLocation="MTBXMLBase.xsd">
</xs:import>

<!-- add annotation/comments -->

 <!-- to hold a MTBSetWeightedEnergy -->

 <xs:complexType name="MTBXMLSetWeightedEnergyType">
    <xs:sequence>
      <xs:element name="energies" type="xs:anyType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="weights" type="xs:double" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
 </xs:complexType>

</xs:schema>
